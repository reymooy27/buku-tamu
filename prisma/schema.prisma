// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Tamu {
  id Int @id @default(autoincrement())
  nama String
  alamat String
  hp Int
  jenisKelamin String
  jamMasuk DateTime @default(now())
  jamKeluar DateTime?
  keterangan String
}